{
    "version": 2,
    "name": "cow-engine",
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/server.js" }
    ],
    "crons": [
      {
        "path": "/src/schedule/notify-cron",
        "schedule": "03 13 * * *"
      }
    ]
  }